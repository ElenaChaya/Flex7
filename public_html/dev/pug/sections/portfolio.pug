mixin portfolioPhoto(title)
    .portfolio-photo__pic
        a(href="static/img/content/portfolio/" +title+".jpg").portfolio-photo_link
            .portfolio-photo__img(style=`background-image: url('static/img/content/portfolio/${title}.jpg')`)

section.portfolio#Portfolio
    .container
        .portfolio-content
            h2.g-section-title.g-section-title--mini Watch the video, and you <br> will understand the style of my work         
            .portfolio-video-slider.slider.video-slider
                button.slider__controlls.portfolio-video-slider__prev
                button.slider__controlls.portfolio-video-slider__next
                button.mobile__controlls.mob-portfolio-video-slider__prev
                button.mobile__controlls.mob-portfolio-video-slider__next
                .js-video-slider.js-portfolio-video-slider
                    .video__item
                        iframe( width="100%" height="315" src="https://www.youtube.com/embed/09R8_2nJtjg?enablejsapi=1" frameborder="0" allowfullscreen id="port-vid-1")
                        
                    .video__item
                        iframe( width="100%" height="315" src="https://www.youtube.com/embed/09R8_2nJtjg?enablejsapi=1" frameborder="0"  allowfullscreen id="port-vid-2")
                        
                    .video__item
                        iframe( width="100%" height="315" src="https://www.youtube.com/embed/09R8_2nJtjg?enablejsapi=1" frameborder="0" allowfullscreen id="port-vid-3")
                        
                    .video__item
                        iframe( width="100%" height="315" src="https://www.youtube.com/embed/09R8_2nJtjg?enablejsapi=1" frameborder="0"  allowfullscreen id="port-vid-4")
                        
                        
            .portfolio-photo
